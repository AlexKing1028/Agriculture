<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.redrock.agriculture.Fragment.RefreshListFragment">
    <com.example.redrock.agriculture.widget.CustomSwipeToRefreshView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/swipe_layout_main_content">

        <com.example.redrock.agriculture.widget.LoadMoreListView
            android:id="@+id/list_main_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        </com.example.redrock.agriculture.widget.LoadMoreListView>
    </com.example.redrock.agriculture.widget.CustomSwipeToRefreshView>
</FrameLayout>
